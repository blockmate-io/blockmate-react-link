{
  "name": "@blockmate.io/blockmate-react-link",
  "version": "3.0.1",
  "description": "Application to link external service to Blockmate Link app",
  "author": "blockmate-io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockmate-io/blockmate-react-link.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle --no-compress --format modern,cjs",
    "start": "microbundle watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "form-data": "^4.0.5",
    "gh-pages": "^6.3.0",
    "microbundle": "^0.15.1",
    "node-notifier": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^0.0.0",
    "rollup": "^2.79.2",
    "tough-cookie": "^6.0.0"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/blockmate-io/blockmate-react-link/issues"
  },
  "homepage": "https://github.com/blockmate-io/blockmate-react-link#readme",
  "keywords": [
    "blockmate",
    "link",
    "crypto",
    "aisp",
    "web3",
    "api",
    "personal",
    "finance",
    "management"
  ],
  "dependencies": {
    "buffer": "^6.0.3"
  }
}
